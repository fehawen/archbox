! vim:ft=xdefaults:

Xft.autohint: 0
Xft.hinting: true
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault
Xft.antialias: 1
Xft.rgba: rgb

Xcursor.size: 12
Xcursor.theme: default

XTerm*termName: xterm-16color
XTerm*internalBorder: 36
XTerm*borderWidth: 0
XTerm*utf8: true
XTerm*scrollbar: false
XTerm*cursorBlink: true
XTerm*selectToClipboard: true
XTerm*trimSelection: true
XTerm*renderFont: true
XTerm*wideChars: true
XTerm*visualBell: false
XTerm*faceName: xft:monospace:size=12
*font: xft:monospace:size=12

XTerm.vt100.translations: #override \n\
	Shift Ctrl <KeyPress> v:insert-selection(CLIPBOARD) \n\
	Shift Ctrl <KeyPress> c:select-set(CLIPBOARD)

*foreground: #181005
*background: #faf0e6

*cursorcolor: #181005

*color0: #f0e6dc
*color8: #aea59c

*color1: #dc143c
*color9: #dc143c

*color2: #006400
*color10: #006400

*color3: #a0522d
*color11: #a0522d

*color4: #4682b4
*color12: #4682b4

*color5: #663399
*color13: #663399

*color6: #008b8b
*color14: #008b8b

*color7: #181005
*color15: #181005
