! vim:ft=xdefaults:

Xft.autohint: 0
Xft.hinting: true
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault
Xft.antialias: 1
Xft.rgba: rgb

Xcursor.size: 12
Xcursor.theme: default

XTerm*termName: xterm-16color
XTerm*internalBorder: 36
XTerm*borderWidth: 0
XTerm*utf8: true
XTerm*scrollbar: false
XTerm*cursorBlink: true
XTerm*selectToClipboard: true
XTerm*trimSelection: true
XTerm*renderFont: true
XTerm*wideChars: true
XTerm*visualBell: false
XTerm*faceName: xft:monospace:size=12
*font: xft:monospace:size=12

XTerm.vt100.translations: #override \n\
	Shift Ctrl <KeyPress> v:insert-selection(CLIPBOARD) \n\
	Shift Ctrl <KeyPress> c:select-set(CLIPBOARD)

*foreground: #b2bebf
*background: #1e282c

*cursorColor: #b2bebf

*color0: #263034
*color8: #566166

*color1: #b56666
*color9: #b56666

*color2: #779687
*color10: #779687

*color3: #b98675
*color11: #b98675

*color4: #5e828f
*color12: #5e828f

*color5: #b48291
*color13: #b48291

*color6: #7ab1b3
*color14: #7ab1b3

*color7: #b2bebf
*color15: #b2bebf
