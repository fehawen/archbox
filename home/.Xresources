! vim:ft=xdefaults:

Xft.autohint: 0
Xft.hinting: true
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault
Xft.antialias: 1
Xft.rgba: rgb

Xcursor.size: 12
Xcursor.theme: default

XTerm*termName: xterm-16color
XTerm*internalBorder: 24
XTerm*borderWidth: 0
XTerm*utf8: true
XTerm*scrollbar: false
XTerm*cursorBlink: true
XTerm*selectToClipboard: true
XTerm*trimSelection: true
XTerm*renderFont: true
XTerm*wideChars: true
XTerm*visualBell: false
XTerm*scaleHeight: 1.2
XTerm*faceName: xft:monospace:size=12
*font: xft:monospace:size=12

XTerm.vt100.translations: #override \n\
	Shift Ctrl <KeyPress> v:insert-selection(CLIPBOARD) \n\
	Shift Ctrl <KeyPress> c:select-set(CLIPBOARD)

*foreground: #24292e
*background: #ffffff

*cursorColor: #24292e

*color0: #f2f2f2
*color8: #969696

*color1: #d73a49
*color9: #d73a49

*color2: #22863a
*color10: #22863a

*color3: #e36209
*color11: #e36209

*color4: #032f62
*color12: #032f62

*color5: #6f42c1
*color13: #6f42c1

*color6: #005cc5
*color14: #005cc5

*color7: #24292e
*color15: #24292e
