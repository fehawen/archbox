! vim:ft=xdefaults:

Xft.autohint: 0
Xft.hinting: true
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault
Xft.antialias: 1
Xft.rgba: rgb

Xcursor.size: 10
Xcursor.theme: default

XTerm*termName: xterm-16color
XTerm*internalBorder: 30
XTerm*borderWidth: 0
XTerm*utf8: true
XTerm*scrollbar: false
XTerm*saveLines: 10000
XTerm*cursorBlink: true
XTerm*selectToClipboard: true
XTerm*trimSelection: true
XTerm*renderFont: true
XTerm*wideChars: true
XTerm*visualBell: false
XTerm*faceName: xft:monospace:size=10
*font: xft:monospace:size=10

XTerm.vt100.translations: #override \n\
    Shift Ctrl <KeyPress> v:insert-selection(CLIPBOARD) \n\
    Shift Ctrl <KeyPress> c:select-set(CLIPBOARD)

*foreground: #9cb9de
*background: #08090c

*cursorColor: #9cb9de

*color0: #090d11
*color8: #30445c

*color1: #e24358
*color9: #e24358

*color2: #9cb9de
*color10: #9cb9de

*color3: #9cb9de
*color11: #9cb9de

*color4: #9cb9de
*color12: #9cb9de

*color5: #9cb9de
*color13: #9cb9de

*color6: #9cb9de
*color14: #9cb9de

*color7: #9cb9de
*color15: #9cb9de
