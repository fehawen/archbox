! vim:ft=xdefaults:

Xft.autohint: 0
Xft.hinting: true
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault
Xft.antialias: 1
Xft.rgba: rgb

Xcursor.size: 12
Xcursor.theme: default

XTerm*termName: xterm-16color
XTerm*internalBorder: 24
XTerm*borderWidth: 0
XTerm*utf8: true
XTerm*scrollbar: false
XTerm*cursorBlink: true
XTerm*selectToClipboard: true
XTerm*trimSelection: true
XTerm*renderFont: true
XTerm*wideChars: true
XTerm*visualBell: false
XTerm*faceName: xft:monospace:pixelsize=12
*font: xft:monospace:pixelsize=12

XTerm.vt100.translations: #override \n\
	Shift Ctrl <KeyPress> v:insert-selection(CLIPBOARD) \n\
	Shift Ctrl <KeyPress> c:select-set(CLIPBOARD)

*.background: #010305
*.foreground: #fff9fa
*.cursorColor: #fff9fa

*.color0: #010305
*.color8: #323e49

*.color1: #af5f00
*.color9: #af5f00

*.color2: #6d727e
*.color10: #6d727e

*.color3: #90909b
*.color11: #90909b

*.color4: #babbba
*.color12: #babbba

*.color5: #90909b
*.color13: #90909b

*.color6: #babbba
*.color14: #babbba

*.color7: #fff9fa
*.color15: #fff9fa
